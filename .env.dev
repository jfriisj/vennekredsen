# Development Environment Variables
# Brug denne fil til lokale udvikling
# Kopiér til .env.dev.local og tilpas dine værdier

# Database Configuration
POSTGRES_DB=vennekredsen_dev
POSTGRES_USER=dev_user
POSTGRES_PASSWORD=dev_password_123

# JWT Security (CHANGE IN PRODUCTION!)
JWT_SECRET=dev_jwt_secret_key_change_in_production_12345
INTERNAL_TOKEN=dev_internal_token_change_in_production_67890

# pgAdmin Configuration (optional)
PGADMIN_EMAIL=admin@dev.local
PGADMIN_PASSWORD=dev_admin_123

# Flask Development Settings
FLASK_ENV=development
FLASK_DEBUG=1
FLASK_APP=app.py

# API Configuration
API_HOST=0.0.0.0
API_PORT=5000

# Frontend Configuration  
FRONTEND_PORT=85

# Default Admin User (created with create-admin command)
DEFAULT_ADMIN_USERNAME=admin
DEFAULT_ADMIN_EMAIL=admin@hashojskolen.dk

# Logging
LOG_LEVEL=DEBUG

# Development URLs (same as production for consistency)
API_BASE_URL=http://localhost:5000
FRONTEND_BASE_URL=http://localhost:85