{
  "extends": ["html-validate:recommended"],
  "rules": {
    "void-style": "off",
    "no-inline-style": "off",
    "require-sri": "off",
    "prefer-native-element": "off",
    "no-trailing-whitespace": "error",
    "doctype-style": "off",
    "no-implicit-button-type": "off"
  },
  "elements": [
    "html5"
  ]
}